{
	"name": "dynamicemotionbasedplaylistgenerator",
	"context": "..",
	"dockerFile": "../Dockerfile",
	
	"runArgs": [
	  "--env", "PYTHONDONTWRITEBYTECODE=1",
	  "--env", "PYTHONUNBUFFERED=1",
	  "--env", "FLASK_APP=app.py"
	],
	
	"postCreateCommand": "pip install -r requirements.txt",
	"remoteUser": "appuser",
	"overrideCommand": true,
	"postStartCommand": "python /tmp/debugpy --wait-for-client --listen 0.0.0.0:5678 -m flask run --no-debugger --no-reload --host 0.0.0.0 --port 5002"
  }
  